    if (searchbox) {
        searchbox.style.display = "none";
    }

    function toggleSearch() {
        var searchbox = document.getElementById("searchbox");

        if (submenu.style.display === "none") {
            submenu.classList.add("fade-in");
            submenu.style.display = "block";
            searchbox.style.display = "none";
        } else {
            submenu.style.display = "none";
            searchbox.classList.add("fade-in");
            searchbox.style.display = "block";
        }
    }

    function changeSubMenuIcon(){
        if(document.getElementById("submenu-m-trigger").innerText == "menu"){
            document.getElementById("submenu-m-trigger").innerText = "close";
        }
        else{
            document.getElementById("submenu-m-trigger").innerText = "menu";
        }
    }</script><div class="dark h-pt-3"><div class="container"><div class="row col-12 col-lg-9"><div class="text-h-primary label position-relative top-3rem"></div></div></div></div><div class="dark h-pt-5 h-pt-lg-0 h-pb-4"><div id="blog-post-container" class="container flex-column flex-lg-row d-flex position-relative"><div class="col-12 col-lg-9"><div id="thumbnail-image" class="d-flex justify-content-center"> <img width="840" height="450" src="https://www.hostinger.com.br/tutoriais/wp-content/uploads/sites/12/2018/04/Como-usar-o-comando-Wget.jpg" class="attachment-1110x454 size-1110x454 wp-post-image" alt="Wget: o que é, como instalar e exemplos de comandos Wget para Linux" loading="lazy" srcset="https://www.hostinger.com.br/tutoriais/wp-content/uploads/sites/12/2018/04/Como-usar-o-comando-Wget.jpg 840w, https://www.hostinger.com.br/tutoriais/wp-content/uploads/sites/12/2018/04/Como-usar-o-comando-Wget-150x80.jpg 150w, https://www.hostinger.com.br/tutoriais/wp-content/uploads/sites/12/2018/04/Como-usar-o-comando-Wget-300x161.jpg 300w, https://www.hostinger.com.br/tutoriais/wp-content/uploads/sites/12/2018/04/Como-usar-o-comando-Wget-768x411.jpg 768w" sizes="(max-width: 840px) 100vw, 840px" /></div><div class="d-flex flex-column flex-lg-row"><div class="col-12 d-flex flex-row align-items-center justify-content-center flex-lg-column justify-content-lg-start
